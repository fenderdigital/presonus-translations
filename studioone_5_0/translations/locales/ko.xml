		<Association key="Jan" value="1월(일월)"/>
		<Association key="Feb" value="2월(이월)"/>
		<Association key="Mar" value="3월(삼월)"/>
		<Association key="Apr" value="4월(사월)"/>
		<Association key="May" value="5월(오월)"/>
		<Association key="Jun" value="6월(유월)"/>
		<Association key="Jul" value="7월(칠월)"/>
		<Association key="Aug" value="8월(팔월)"/>
		<Association key="Sep" value="9월(구월)"/>
		<Association key="Oct" value="10월(시월)"/>
		<Association key="Nov" value="11월(십일월)"/>
		<Association key="Dec" value="12월(십이월)"/>
	</Attributes>
</LocaleInfo>
