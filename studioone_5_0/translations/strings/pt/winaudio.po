#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Audio Playback is disabled."
msgstr "A Reprodução de áudio está desativada."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Audio Recording is disabled."
msgstr "A Gravação de áudio está desativada"

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Failed to open %(1)!"
msgstr "%(1) Falhou ao abrir!"

#: .\winaudiomanager.cpp
msgid "[WindowsAudio]Manage audio devices"
msgstr "Gerenciar dispositivos de áudio"

#: .\wasapiclient.cpp
msgid "[WindowsAudio]None (Recording disabled)"
msgstr "Nada (Gravação desativada)"

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Please make sure that the sample rates of your recording and playback devices match."
msgstr "Certifique-se de que as taxas de amostragem dos dispositivos de gravação e reprodução são iguais."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Please make sure that the speaker configuration is set to stereo."
msgstr "Certifique-se de que a configuração dos falantes é estéreo."

#: .\winaudiomain.cpp
msgid "[WindowsAudio]Support for Consumer Audio Devices on Windows"
msgstr "Suporte a Dispositivos de Áudio de Consumidor do Windows"

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The Windows audio service is not running on this system."
msgstr "O serviço de áudio do Windowsnão está sendo executado neste sistema."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The requested audio format is not supported by this device."
msgstr "O formato de áudio solicitado não é compatível com este dispositivo."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The selected audio device is in use by another application."
msgstr "O dispositivo de áudio selecionado está em uso por outro aplicativo."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The selected audio device is no longer available."
msgstr "O dispositivo de áudio selecionado não está mais disponível."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Unable to open device %(1) in exclusive mode. Falling back to shared mode."
msgstr "Não foi possível abrir o dispositivo %(1) em modo exclusivo. Retornando ao modo compartilhado."

#: .\plugversion.h
msgid "[WindowsAudio]Windows Audio Support"
msgstr "Suporte a Áudio do Windows"

