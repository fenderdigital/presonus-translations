#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Audio Playback is disabled."
msgstr "La reproducción de audio está deshabilitada."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Audio Recording is disabled."
msgstr "La grabación de audio está deshabilitada."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Failed to open %(1)!"
msgstr "Fallo al abrir %(1)!"

#: .\winaudiomanager.cpp
msgid "[WindowsAudio]Manage audio devices"
msgstr "Administrar dispositivos de audio"

#: .\wasapiclient.cpp
msgid "[WindowsAudio]None (Recording disabled)"
msgstr "Ninguno (Grabación desactivada)"

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Please make sure that the sample rates of your recording and playback devices match."
msgstr "Por favor asegúrese que las frecuencias de muestreo para sus dispositivos de grabación y reproducción coincidan."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Please make sure that the speaker configuration is set to stereo."
msgstr "Por favor asegúrese que la configuración de altavoces esté establecida en estéreo."

#: .\winaudiomain.cpp
msgid "[WindowsAudio]Support for Consumer Audio Devices on Windows"
msgstr "Soporte para dispositivos de audio en Windows"

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The Windows audio service is not running on this system."
msgstr "El servicio de audio de Windows no funciona en este sistema."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The requested audio format is not supported by this device."
msgstr "Este dispositivo no soporta el formato de audio solicitado."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The selected audio device is in use by another application."
msgstr "El dispositivo de audio seleccionado está siendo utilizado por otra aplicación."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The selected audio device is no longer available."
msgstr "El dispositivo de audio seleccionado ya no está disponible."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Unable to open device %(1) in exclusive mode. Falling back to shared mode."
msgstr "No se pudo abrir el dispositivo %(1) en modo exclusivo. Volviendo al modo compartido."

#: .\plugversion.h
msgid "[WindowsAudio]Windows Audio Support"
msgstr "Soporte de audio de Windows"

