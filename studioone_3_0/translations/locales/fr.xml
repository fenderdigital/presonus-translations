		<Association key="Sat" value="samedi"/>
	</Attributes>	
	
	<Attributes x:id="months">
		<Association key="Jan" value="janvier"/>
		<Association key="Feb" value="février"/>
		<Association key="Mar" value="mars"/>
		<Association key="Apr" value="avril"/>
		<Association key="May" value="mai"/>
		<Association key="Jun" value="juin"/>
		<Association key="Jul" value="juillet"/>
		<Association key="Aug" value="août"/>
		<Association key="Sep" value="septembre"/>
		<Association key="Oct" value="octobre"/>
		<Association key="Nov" value="novembre"/>
		<Association key="Dec" value="décembre"/>
	</Attributes>
</LocaleInfo>
