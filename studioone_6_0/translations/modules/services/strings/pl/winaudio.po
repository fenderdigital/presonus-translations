#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Audio Playback is disabled."
msgstr "Odtwarzanie audio jest wyłączone."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Audio Recording is disabled."
msgstr "Nagrywanie audio jest wyłączone."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Failed to open %(1)!"
msgstr "Nie udało się otworzyć %(1)!"

#: .\winaudiomanager.cpp
msgid "[WindowsAudio]Manage audio devices"
msgstr "Zarządzaj urządzeniami audio"

#: .\wasapiclient.cpp
msgid "[WindowsAudio]None (Recording disabled)"
msgstr "Brak (wyłączone nagrywanie)"

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Please make sure that the sample rates of your recording and playback devices match."
msgstr "Sprawdź czy częstotliwość próbkowania nagrywania i odtwarzania jest zgodna z częstotliwością urządzenia."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Please make sure that the speaker configuration is set to stereo."
msgstr "Sprawdź czy konfiguracja głośników jest ustawiona na stereo."

#: .\winaudiomain.cpp
msgid "[WindowsAudio]Support for Consumer Audio Devices on Windows"
msgstr "Obsługa urządzeń audio pod Windows"

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The Windows audio service is not running on this system."
msgstr "Usługa Windows audio nie jest uruchomiona na tym systemie."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The requested audio format is not supported by this device."
msgstr "Żądany format audio nie jest obsługiwany przez to urządzenie."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The selected audio device is in use by another application."
msgstr "Wybrane urządzenie audio jest używane przez inną aplikację."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]The selected audio device is no longer available."
msgstr "Wybrane urządzenie audio nie jest już dostępne."

#: .\winaudiodevice.cpp
msgid "[WindowsAudio]Unable to open device %(1) in exclusive mode. Falling back to shared mode."
msgstr "Nie można otworzyć urządzenia %(1) w trybie wyłączności. Powrót do trybu współdzielonego."

#: .\plugversion.h
msgid "[WindowsAudio]Windows Audio Support"
msgstr "Obsługa Windows Audio"

