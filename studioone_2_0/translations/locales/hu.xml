		<Association key="Mar" value="március"/>
		<Association key="Apr" value="április"/>
		<Association key="May" value="május"/>
		<Association key="Jun" value="június"/>
		<Association key="Jul" value="július"/>
		<Association key="Aug" value="augusztus"/>
		<Association key="Sep" value="szeptember"/>
		<Association key="Oct" value="október"/>
